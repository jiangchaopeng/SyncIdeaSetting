<templateSet group="user">
  <template name="/**" value="/**&#10;* @author $name$&#10;* @description $dest$&#10;* @date $DATE$ &#10;*/" description="快速产生注释" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="annotated(&quot;annotation qname&quot;)" defaultValue="jcp" alwaysStopAt="true" />
    <variable name="dest" expression="decapitalize(String)" defaultValue="" alwaysStopAt="true" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>